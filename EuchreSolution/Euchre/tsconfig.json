{
	"compilerOptions": {
		"target": "es2015",
		"sourceMap": true,
		"noFallthroughCasesInSwitch": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"removeComments": true,
		"strict": true,
		"plugins": [
			{
				"name": "tslint-language-service",
			},
		],
	},
	"compileOnSave": true,
}