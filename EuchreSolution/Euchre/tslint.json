{
	"extends": "tslint:recommended",
	"rules": {
		"align": [
			true,
			"elements",
			"members",
			"statements"
		],
		"array-type": [
			true,
			"array"
		],
		"arrow-parens": [
			true,
			"ban-single-arg-parens"
		],
		"comment-format": [
			false
		],
		"eofline": false,
		"indent": [
			true,
			"tabs"
		],
		"interface-name": [
			true,
			"never-prefix"
		],
		"member-ordering": [
			false
		],
		"object-literal-sort-keys": [
			false
		],
		"only-arrow-functions": [
			false
		],
		"space-before-function-paren": [
			true,
			{
				"anonymous": "always",
				"constructor": "never",
				"method": "never",
				"named": "never"
			}
		],
		"strict-boolean-expressions": [
			true,
			"allow-null-union",
			"allow-undefined-union",
			"allow-string",
			"allow-mix"
		],
		"switch-default": false,
		"variable-name": [
			true,
			"ban-keywords"
		]
	}
}
